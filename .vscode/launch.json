{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Train.py with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--async_distributed",
                "--available_gpus", "4,5,6,7",
                "--num_actors", "4",
                "--algo_name", "IMPALA",
                "--env_name", "meltingpot",
                "--map_name", "predator_prey__open"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "4,5,6,7"
            }
        }
    ]
}
